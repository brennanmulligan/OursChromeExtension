{
  "manifest_version": 3,
  "name": "Ours - Time Tracker",
  "description": "Track how much time is spent on sites",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
  }
  "icons": {
    "16": "photos/OursIcon16.png",
    "32": "photos/OursIcon32.png",
    "48": "photos/OursIcon48.png",
    "128": "photos/OursIcon128.png"
  }
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://developer.chrome.com/docs/extensions/*",
        "https://developer.chrome.com/docs/webstore/*"
      ]
    }
  ]
}
